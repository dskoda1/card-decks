{
    "extends": "eslint:recommended",
    "parserOptions": {
         "ecmaVersion": 5,
         "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "strict": [
            "off"
        ],
        "no-undef": [
            "off"
        ]
    }
}